{
  "name": "kirbitz.github.io",
  "version": "0.0.1",
  "description": "My portfolio website to display technologies and projects that I have worked on",
  "main": "index.js",
  "private": "true",
  "type": "module",
  "scripts": {
    "start": "node ./server/index.js",
    "devServe": "nodemon --watch server/index.js --watch server/api/dataHelper.js server/index.js",
    "client": "esbuild --bundle --minify --target=ES6 --outfile=public/bundle.js client/app.jsx",
    "devClient": "nodemon --watch client --exec ./node_modules/.bin/esbuild --bundle --sourcemap --target=ES6 --outfile=public/bundle.js client/app.jsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kirbitz/kirbitz.github.io.git"
  },
  "keywords": [
    "Portfolio",
    "Kirbitz"
  ],
  "author": "Tyler Marefke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kirbitz/kirbitz.github.io/issues"
  },
  "homepage": "https://github.com/Kirbitz/kirbitz.github.io#readme",
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4"
  },
  "dependencies": {
    "esbuild": "^0.14.38",
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
